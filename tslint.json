{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    //"jsRules": {},
    "rules": {
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": [true, 2],
        "no-console": false,
        "variable-name": [
            true,
            "check-format",
            "allow-leading-underscore",
            "ban-keywords"
        ],
        "ordered-imports": false
    }
}